<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/search_list_navigation"
    app:startDestination="@id/searchListFragment">

    <fragment
        android:id="@+id/searchListFragment"
        android:name="com.onurcem.feature.search_list.presentation.SearchListFragment"
        android:label="fragment_search_list"
        tools:layout="@layout/fragment_search_list" />

    <argument
        android:name="searchId"
        android:defaultValue=""
        app:argType="string"
        app:nullable="false" />

    <deepLink app:uri="onurcem://search_list/?searchId={searchId}" />

</navigation>